# .env file for Ultima Bot broker APIs and configuration

# === Coinbase ===
COINBASE_API_KEY=your_coinbase_api_key
COINBASE_API_SECRET=your_coinbase_api_secret
COINBASE_API_PASSPHRASE=your_coinbase_api_passphrase

# === Binance ===
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret

# === Kraken ===
KRAKEN_API_KEY=your_kraken_api_key
KRAKEN_API_SECRET=your_kraken_api_secret

# === E*TRADE ===
ETRADE_CONSUMER_KEY=your_etrade_consumer_key
ETRADE_CONSUMER_SECRET=your_etrade_consumer_secret
ETRADE_OAUTH_TOKEN=your_etrade_access_token
ETRADE_OAUTH_SECRET=your_etrade_access_secret

# === Robinhood ===
ROBINHOOD_USERNAME=your_robinhood_email
ROBINHOOD_PASSWORD=your_robinhood_password

# === Interactive Brokers ===
IBKR_SESSION_TOKEN=your_ibkr_session_token

# === General Config ===
ENV=development
LOG_LEVEL=INFO
TRADING_MODE=live
SAFE_MODE=true
MAX_POSITION_PCT=0.30
MAX_DRAWDOWN_PCT=0.28
